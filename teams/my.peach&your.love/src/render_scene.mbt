fn render_talk() -> Unit{
@wasm4.set_palette(1, @wasm4.rgb(0xd3d3d3)) // 浅灰
@wasm4.set_palette(2, @wasm4.rgb(0x000000)) // 黑
@wasm4.set_palette(3, @wasm4.rgb(0xffffff)) // 白  
@wasm4.set_palette(4, @wasm4.rgb(0x696969)) // 暗灰   
@wasm4.set_draw_colors(3,index = 1)
@wasm4.set_draw_colors(2,index = 2)
@wasm4.set_draw_colors(1,index = 3)
@wasm4.set_draw_colors(0,index = 4)
if pos.ui_x <= -80 && pos.ui_x1 >= 160{
game.dialogue += 1
if game.dialogue > 60{
    @wasm4.blit(talks,0,132,160,28,talkflag)
}
if game.dialogue > 360{
    @wasm4.blit(talks1,0,132,160,28,talkflag1)
}
if game.dialogue > 660{
    @wasm4.blit(talks2,0,132,160,28,talkflag2)
}
if game.dialogue > 960{
    game.dialogue = 0
    game.stop_dialogue = 1
}
}
}

