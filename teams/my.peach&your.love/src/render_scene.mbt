fn render_ui() -> Unit{
@wasm4.set_palette(1, @wasm4.rgb(0xd3d3d3)) // 浅灰
@wasm4.set_palette(2, @wasm4.rgb(0x000000)) // 黑
@wasm4.set_palette(3, @wasm4.rgb(0xffffff)) // 白  
@wasm4.set_palette(4, @wasm4.rgb(0x696969)) // 暗灰 
@wasm4.set_draw_colors(3,index=1)
@wasm4.set_draw_colors(4,index=2)
@wasm4.set_draw_colors(1,index=3)
@wasm4.set_draw_colors(2,index=4)
@wasm4.blit(uis,pos.ui_x,0,80,160,uiflag)
@wasm4.blit(uis1,pos.ui_x1,0,80,160,uiflag1)
if @wasm4.get_gamepad(index = 1).button_1{
    game.play_ui += 1
}
if game.play_ui >= 1{
    if pos.ui_x > -80  {
      if game.delay_ui_left <= 4{
            game.delay_ui_left += 1
     }
        pos.ui_x -= 1
        game.delay_ui_left = 0
    }
    if pos.ui_x1 < 160{
      if game.delay_ui_right <= 4{
            game.delay_ui_right += 1
     }
       pos.ui_x1 += 1
       game.delay_ui_right = 0
    }
}

}

